<script type="text/javascript">
var res,q;
function generate()
{
 if(document.f1.fname.value=="")
 window.alert("please fill the value of first name");
 return false();

 var gem=parseInt(document.f1.pas.value);
 res= " The total you have to pay for your trip is" +
gem*1000;
 if (gem>5)
 window.alert("sorry,we dont have these many tickets
available");
 else
 window.alert(res);
 if(document.f1.sname.value=="")
 window.alert("please fill the value of second name");
 return false();

 if(document.f1.age.value=="")
 window.alert("please fill the value of your age");
 return false();

 if(document.f1.d1.value=="")
 window.alert("please fill the date you want to
travel");
 return false();


 if(document.f1.pas.value=="")
 window.alert("please fill the number of passengers
travelling");
 return false();

}
</script>
